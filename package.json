{
  "name": "central-gummy-api",
  "version": "1.0.0",
  "description": "API Node.js para integração Shopify-OMIE ES",
  "main": "src/server.js",
  "scripts": {
   
    "dev": "NODE_ENV=development node -r dotenv/config src/server.js dotenv_config_path=.env-dev",
    "dev2": "nodemon --exec sucrase-node src/server.js",
    "prod": "NODE_ENV=production node src/server.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t central-gummy-api .",
    "docker:run": "docker run -p 3000:3000 central-gummy-api"
  },
  "keywords": [
    "api",
    "nodejs",
    "shopify",
    "omie",
    "integration",
    "e-commerce"
  ],
  "author": "Antonio Duarte",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "bull": "^4.12.2",
    "bull-board": "^2.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "express-validator": "^7.0.1",
    "gh": "^2.8.9",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pm2": "^6.0.8",
    "redis": "^4.6.10",
    "sequelize": "^6.35.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.57.0",
    "@flydotio/dockerfile": "^0.7.10",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sucrase": "^3.35.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
